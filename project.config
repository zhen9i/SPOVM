[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Evgenii (zhen9i)
[access "refs/for/refs/*"]
	read = group user/Evgenii (zhen9i)
	push = group user/Evgenii (zhen9i)
	label-Code-Review = -2..+2 group user/Evgenii (zhen9i)
	label-Verified = -1..+1 group user/Evgenii (zhen9i)
	rebase = group user/Evgenii (zhen9i)
	removeReviewer = group user/Evgenii (zhen9i)
	submit = group user/Evgenii (zhen9i)
[access "refs/heads/*"]
	read = group user/Evgenii (zhen9i)
	create = group user/Evgenii (zhen9i)
	push = +force group user/Evgenii (zhen9i)
	pushMerge = group user/Evgenii (zhen9i)
	label-Code-Review = -2..+2 group Registered Users
	labelAs-Verified = -1..+1 group Registered Users
[access "refs/tags/*"]
	push = +force group user/Evgenii (zhen9i)
	pushTag = group user/Evgenii (zhen9i)
	pushSignedTag = group user/Evgenii (zhen9i)
